        "text": {
          "headline": "Sunday, Octo. 18, 1835.",
          "text": "<div>\n\t\t\t<div>\n\t\t\t\t<div>\n\t\t\t\t\t<p>At 4 o'clock, came to at Owensboro, or Yellow Banks.\n</p>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</div>"
        }
